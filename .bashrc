# .bashrc
alias ll="ls -la"

# Git shortcuts
alias gis="git status"
alias gid="git difftool"
alias gim="git mergetool"
alias gil='git log --format="%C(yellow)%H %C(red)%cr %Cgreen%an %C(cyan)%s %C(magenta)%d"'
alias gidbranches="git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative"
alias gill='git diff-tree --no-commit-id --name-only -r'

# Shortcuts
alias cde="cd ~/Documents/Qlik/Sense/Extensions"
